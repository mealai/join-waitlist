/********************************************
  Landing Page Scroll Sections
*********************************************/
.landingPage {
	// If you want no extra spacing around, ensure margin/padding is zero
	margin: 0;
	padding: 0;
}
  
.landing-section {
	/* Force each section to be at least full viewport height 
	   so that user scrolling visibly transitions from one 
	   to the next “scene.” 
	*/
	min-height: 100vh; 
	display: flex;
	align-items: center;     /* Center content vertically */
	justify-content: center; /* Center content horizontally (optional) */
	text-align: center;      /* Just an example for text alignment */
}
  
.section-content {
	/* A container to hold the phone and text side by side, etc. */
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: center;
}
  
